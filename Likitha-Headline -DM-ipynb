   "source": [
    "def word_p(df, vocab):\n",
    "    \n",
    "    p_wrd_d = {}\n",
    "    for word in tqdm(vocab):\n",
    "        wrd_o = 0\n",
    "        for sen in df['headline'].values:\n",
    "            if(word in sen):\n",
    "                wrd_o = wrd_o + 1\n",
    "        p_wrd_d[word] = wrd_o / df.shape[0]\n",
    "    \n",
    "#     print(\"---Word Probability in Documents ---\")\n",
    "#     print(p_wrd_d)\n",
    "    return p_wrd_d"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "id": "1f88615e",
   "metadata": {},
   "outputs": [],
   "source": [
    "def con_pro(df, vocab, smt):\n",
    "    \n",
    "\n",
    "    cats = list(set(df['category'].values))\n",
    "    cat_co = df.groupby(by='category').count()['headline'].to_dict()\n",
    "\n",
    "\n",
    "    p_wrd = {}\n",
    "    \n",
    "    pr_wrd = {}\n",
    "\n",
    "    for c_n in tqdm(cats):\n",
    "        p_wrd[c_n] = {}\n",
    "        for word in list(vocab):\n",
    "            wrd_con = 0\n",
    "            for sen in df[df['category'] == c_n]['headline'].values:\n",
    "                if(word in sen):\n",
    "                    wrd_con = wrd_con + 1\n",
    "            \n",
    "            in_val = (wrd_con + smt) / (cat_co[c_n] + (smt*len(cat_co)))\n",
    "            p_wrd[c_n][word] = in_val\n",
    "            \n",
    "    \n",
    "    \n",
    "    return p_wrd\n",
    "        \n",
    "    "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "id": "c6c1f500",
   "metadata": {},
   "outputs": [],
   "source": [
    "def calc_cp(df):\n",
    "    \n",
    "    cat_co = df.groupby(by='category').count()['headline'].to_dict()\n",
    "    \n",
    "    for key,value in cat_co.items():\n",
    "        cat_co[key] = (value / df.shape[0])\n",
    "    \n",
    "#     print(\"--- Prior Probability of the Categories ---\")\n",
    "#     print(cat_co)\n",
    "    return cat_co"
   ]
  },
