    "print(\"--- Conditional Probabilities of The Word in The Category ---\")\n",
    "\n",
    "for cat in c_pr.keys():\n",
    "    \n",
    "    for i,j in zip(c_pr[cat].keys(),c_pr[cat].values()):\n",
    "        \n",
    "        if(j != 0.0):\n",
    "            print(cat, \"-\", i, \"-\", j)\n",
    "        "
   ]
