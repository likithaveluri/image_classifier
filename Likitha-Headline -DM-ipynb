   "id": "7c159ca3",
   "metadata": {},
   "outputs": [],
   "source": [
    "def gen_vocab(df):\n",
    "    vocab_dic = {}\n",
    "    vocab = set()\n",
    "    \n",
    "    l_f = []\n",
    "    \n",
    "    for i in range(0,len(df['headline'].values)):\n",
    "        temp = set(df['headline'].values[i].split(\" \"))\n",
    "        vocab.update(temp)\n",
    "    \n",
    "    \n",
    "\n",
    "    for i in tqdm(vocab.copy()):\n",
    "        count = 0\n",
    "        for sen in df['headline'].values:\n",
    "            if(i in sen):\n",
    "                count = count + 1\n",
    "        vocab_dic[i] = count\n",
    "        if(count <= 5):\n",
    "            vocab.remove(i)\n",
    "            del vocab_dic[i]\n",
    "            l_f.append(i)\n",
    "            \n",
    "    with open('vocab.pkl', 'wb') as f:\n",
    "        pkl.dump(vocab,f)\n",
    "        \n",
    "#     print(\"----Frequent Words-----\")\n",
    "#     print(vocab_dic)\n",
    "#     print(\"----InFrequent Words-----\")\n",
    "#     print(l_f)\n",
    "    try:\n",
    "        vocab.remove('')\n",
    "        del v_dic['']\n",
    "    except:\n",
    "        pass\n",
    "    \n",
    "    return vocab,vocab_dic, l_f"
   ]
  },
