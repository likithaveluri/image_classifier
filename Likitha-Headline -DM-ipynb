    "def predict(t_df, c_pr, wrd_p, cl_pr):\n",
    "    \n",
    "    test_data = []\n",
    "    \n",
    "    test_labels = list(t_df['category'].values)\n",
    "    \n",
    "    pred_labels = []\n",
    "    \n",
    "    t_headings = t_df['headline'].values\n",
    "    \n",
    "    for t_h in t_headings:\n",
    "        temp = t_h.split(\" \")\n",
    "        test_data.append(temp)\n",
    "    \n",
    "    for t_v in test_data:\n",
    "        pred_cls = {}\n",
    "\n",
    "        for key,val in c_pr.items():\n",
    "            num_a = 1\n",
    "            den = 1\n",
    "            for t_wrd in t_v:\n",
    "                if(t_wrd in val.keys()):\n",
    "                    num_a = num_a * val[t_wrd]\n",
    "                if(t_wrd in wrd_p.keys()):\n",
    "                    den = den * wrd_p[t_wrd]\n",
    "            \n",
    "            num = num_a * cl_pr[key]\n",
    "    \n",
    "            pred_cls[key] = num / den\n",
    "        \n",
    "        pred_labels.append(max(pred_cls, key= pred_cls.get))\n",
    "    \n",
    "    acc_count = 0\n",
    "    \n",
    "    for k,j in zip(test_labels,pred_labels):\n",
    "        \n",
    "        if(k == j):\n",
    "            acc_count += 1\n",
    "            \n",
    "    \n",
    "    return acc_count / len(test_labels)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "id": "c8495bab",
   "metadata": {
    "scrolled": true
   },
   "outputs": [],
   "source": [
    "\n",
    "def cross_val(df, smth):\n",
    "    \n",
    "    kfold = 5\n",
    "\n",
    "    f_s = len(df)/5\n",
    "\n",
    "    acc = []\n",
    "\n",
    "    for i in range(kfold):\n",
    "    \n",
    "        testing = df.iloc[int(i*f_s):int((i+1)*f_s), :]\n",
    "        df_a = df.iloc[:int(i*f_s),:]\n",
    "        df_b = df.iloc[int(f_s*(i+1)):,:]\n",
    "        training = pd.concat([df_a, df_b])\n",
    "    \n",
    "        vocab, v_dic, l_f = gen_vocab(training)\n",
    "        \n",
    "        wrd_p = word_p(df=training, vocab=vocab)\n",
    "        c_pr = con_pro(df=training, vocab=vocab, smt=smth)\n",
    "        cl_pr = calc_cp(df=training)\n",
    "    \n",
    "        accuracy = predict(testing, c_pr, wrd_p, cl_pr)\n",
    "        \n",
    "        acc.append(accuracy)\n",
    "        \n",
    "#     print(\"Accuracy for 5 Folds:\", acc)\n",
    "    m_acc = sum(acc)/len(acc)\n",
    "    \n",
    "    \n",
    "    \n",
    "    return v_dic, l_f, wrd_p, c_pr, cl_pr, acc, m_acc"
   ]
  },
